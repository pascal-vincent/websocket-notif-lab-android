<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fr.skalit.websocketnotificationpoc">


    <application
        android:name="WsNotifApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name="fr.skalit.websocketnotificationpoc.service.AlertService"
            android:enabled="true"
            android:exported="false"
            android:icon="@drawable/ic_action_play_over_video"
            android:permission="fr.skalit.websocketnotificationpoc.service.AlertService" />

        <service
            android:name=".service.AlertMessageManager"
            android:exported="false" />

    </application>

    <uses-permission android:name="android.permission.INTERNET" />
    <permission android:name="fr.skalit.websocketnotificationpoc.service.AlertService" />
    <uses-permission android:name="fr.skalit.websocketnotificationpoc.service.AlertService" />

</manifest>